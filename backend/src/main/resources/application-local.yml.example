# 本地开发环境配置文件示例
# 复制此文件为 application-local.yml 并填入你的实际配置
# application-local.yml 已在 .gitignore 中，不会被提交到 Git

spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/mootai
    username: postgres
    password: your-local-database-password
    driver-class-name: org.postgresql.Driver

jwt:
  secret: your-local-jwt-secret-key
  expiration: 86400000 # 24小时（毫秒）

